    "dist/",
    "README.md",
    "LICENSE"
  ],
  "dependencies": {
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "axios": "^1.6.2",
    "chalk": "^5.3.0"
  },
  "devDependencies": {
    "typescript": "^5.3.2",
