  "version": "1.0.0",
  "description": "CLI tool for managing SAP Work Zone HTML5 content provider cache refresh",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "workzonetools": "./dist/index.js"
  },
  "scripts": {
